void setup() {
  Serial.begin(9600);

  pinMode(pcswitchPin, OUTPUT);
  pinMode(localswitchPin, INPUT_PULLUP);
  pinMode(commsOutPin, OUTPUT);
  pinMode(commsInPin, INPUT_PULLUP);

  // SSD1306_SWITCHCAPVCC = generate display voltage from 3.3V internally
  if (!display.begin(SSD1306_SWITCHCAPVCC, SCREEN_ADDRESS)) {
    Serial.println(F("SSD1306 allocation failed"));
    for (;;); // Don't proceed, loop forever
  }

  // Clear the buffer
  display.clearDisplay();
  display.setTextColor(SSD1306_WHITE);

  // Draw a single pixel in white
  // display.drawPixel(10, 10, SSD1306_WHITE);

  // Show the display buffer on the screen. You MUST call display() after
  // drawing commands to make them visible on screen!

  // drawcustombitmap();
  drawSplash();
  display.display();

  Serial.flush();
  delay(500);
  Serial.println(F("Screen module"));
  Serial.flush();

  display.clearDisplay();
  drawPcWaiting();
  display.display();

  delay(2500);
  char rec[32];
  if (Serial.peek() == 'u') {
    Serial.read();

    display.clearDisplay();
    drawPcSuccess();
    display.display();

    switch (Serial.read()) {
      case 'a':
        screensaver = 0;
        // drawCenteredText("screensaver");

        break;
      case 't':
        screensaver = 1;
        // drawCenteredText("time");

        Serial.write("k");
        delay(2000);
        repeats = 0;
        while (Serial.available() > 0) {
          rec[repeats] = Serial.read();
          drawCenteredText(rec, true);
          delay(100);
          repeats++;
        }
        rec[repeats] = '\0';
        Serial.println("final value is " + String(atol(rec)));
        milisAtStart = (atol(rec) * 1000L) - millis();
        break;
      case 'd':
        screensaver = 2;
        // drawCenteredText("date");

        Serial.print('k');
        Serial.flush();
        delay(2000);
        repeats = 0;
        while (Serial.available() > 0) {
          rec[repeats] = Serial.read();
          drawCenteredText(rec, true);
          delay(100);
          repeats++;
        }
        rec[repeats] = '\0';
        Serial.println("final value is " + String(atol(rec)));
        milisAtStart = (atol(rec) * 1000L) - millis();


        Serial.println(F("Clearing Serial buffer..."));
        repeats = 0;
        while (Serial.available() > 0) {
          Serial.read();
          repeats++;
        }
        Serial.println("Cleared " + String(repeats) + " chars");

        delay(1000);
        Serial.print('k');
        Serial.flush();
        delay(1000);

        while (Serial.available() < 1) {
          delay(0);
        }

        char inc[11];
        repeats = 0;
        while (Serial.available() > 0) {
          inc[repeats] = Serial.read();
          draw11pCenteredText(inc, true);
          delay(100);
          repeats++;
        }

        inc[repeats] = '\0';
        String incString = inc;
        Serial.println("Date received as: " + incString);
        yearAtStart = atol(incString.substring(0, 4).c_str());
        monthAtStart = atol(incString.substring(5, 7).c_str());
        dayAtStart = atol(incString.substring(8, 10).c_str());
        Serial.println("Date parsed as: " + String(yearAtStart) + "." + String(monthAtStart) + "." + String(dayAtStart));
    }

    delay(1500);

    display.clearDisplay();
    drawPcSuccess();
    display.display();
  }
  else {
    display.clearDisplay();
    drawPcDisconnected();
    display.display();
    for (byte i = 0; i < 13; i++) {
      display.invertDisplay(i % 2);
      display.display();
      delay(250);
    }
  }

  delay(1000);

  //display.invertDisplay(0);
  //display.display();

  // display.display() is NOT necessary after every single drawing command,
  // unless that's what you want...rather, you can batch up a bunch of
  // drawing operations and then update the screen all at once by calling
  // display.display(). These examples demonstrate both approaches...


  //TODO: drawcustomtext();

  //testdrawbitmap();    // Draw a small bitmap image

  //testdrawchar();      // Draw characters of the default font

  //testdrawstyles();    // Draw 'stylized' characters
  //digitalWrite(pcswitchPin, HIGH);
}

void loop() {

  display.clearDisplay();
  pcswitch = digitalRead(localswitchPin);
  // pcswitch = 0 if pushed, 1 if released

  if (pcswitch == 1) {
    if (frame > 80) frame = frame - 80;

    if (timestamp < 100) {

      display.clearDisplay();
      drawNanoSleep();
      display.display();

      timestamp++;
    }
    else {
      if (screensaver == 1) {
        doClock();
      }
      else if (screensaver == 2) {
        doCalendar();
      }
      else {
        if (x > 117 || x < 1) {
          goingright = !goingright;
        }
        if (y > 21  || y < 1) {
          goingup    = !goingup;
        }

        if (goingright == true) {
          x = x + 1;
        } else {
          x = x - 1;
        }

        if (goingup == true) {
          y = y + 1;
        } else {
          y = y - 1;
        }

        display.clearDisplay();
        display.drawBitmap(x, y, amogus[frame % 6], 10, 10, WHITE);
        frame++;
        display.display();
        delay(30);
      }
    }



  } else {
    Serial.println(F("pushed"));

    digitalWrite(pcswitchPin, HIGH);
    pcswitch = digitalRead(localswitchPin);
    while (pcswitch == 0) {
      display.clearDisplay();
      drawButtonPushedBig();
      display.display();
      delay(50);
      pcswitch = digitalRead(localswitchPin);
    }
    digitalWrite(pcswitchPin, LOW);
    commsIn = digitalRead(commsInPin);
    Serial.print("commsIn: ");
    Serial.println(commsIn);
    if (commsIn == 1) {
      Serial.println("audio module not ready yet");
      // Not ready to play sound, show error message and continue
      frame = 0;
      while (frame < 7) {
        display.clearDisplay();
        drawNotReady(frame % 2);
        // display.drawBitmap(0, 0, notready[frame % 2], 128, 32, WHITE);
        display.display();
        delay(500);
        frame++;
      }
      frame = 0;
    } else {
      frame = 89;
      display.clearDisplay();
      drawContactingNano();
      display.display();
      digitalWrite(commsOutPin, HIGH);
      while (commsIn == 0) {
        if (frame % 89 == 0) {
          display.clearDisplay();
          drawContactingNano();
          display.display();
        } if (frame == 255) {
          Serial.print("request timed out");
          frame = 0;
          while (frame < 7) {
            display.clearDisplay();
            drawTimedOut(frame % 2);
            // display.drawBitmap(0, 0, notready[frame % 2], 128, 32, WHITE);
            display.display();
            delay(500);
            frame++;
          }
          return;
        }
        display.drawPixel((frame % 89) + 18, 13, BLACK);
        display.drawPixel((frame % 89) + 19, 13, WHITE);
        display.drawPixel((frame % 89) + 20, 13, WHITE);
        display.drawPixel((frame % 89) + 21, 13, WHITE);

        display.drawBitmap(59, 7, wait_shadow, 11, 11, BLACK);
        display.drawBitmap(59, 7, wait, 11, 11, WHITE);

        display.display();

        frame++;
        commsIn = digitalRead(commsInPin);
      }
      // connection established, playing sound...
      digitalWrite(commsOutPin, LOW);
    }
    delay(250);
    timestamp = 0;
  }

}

